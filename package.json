{
  "name": "white-summoner",
  "version": "0.1.4",
  "description": "A user generator with realistic anagraphic data",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha lib/**/*/**/__tests/*.spec.js --recursive",
    "test:integration": "mocha tests/integration/*.spec.js",
    "build:deptree": "depcruise --exclude '^node_modules' --output-type dot lib | dot -T svg > dependencygraph.svg"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ugobriasco/white-summoner.git"
  },
  "keywords": [
    "persona",
    "user",
    "generator",
    "fintech",
    "ecommerce",
    "kyc",
    "testing"
  ],
  "author": "Ugo Briasco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ugobriasco/white-summoner/issues"
  },
  "homepage": "https://github.com/ugobriasco/white-summoner#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "dependency-cruiser": "^4.22.0",
    "husky": "^2.5.0",
    "lint-staged": "^8.2.1",
    "mocha": "^6.1.4"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "faker": "^4.1.0",
    "lodash.deburr": "^4.1.0",
    "random-birthday": "^1.0.1"
  }
}
