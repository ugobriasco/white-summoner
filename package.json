    "url": "git+https://github.com/ugobriasco/summoner.git"
  },
  "keywords": [
    "persona"
  ],
  "author": "Ugo Briasco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ugobriasco/summoner/issues"
  },
  "homepage": "https://github.com/ugobriasco/summoner#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "dependency-cruiser": "^4.22.0",
    "mocha": "^6.1.4",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "faker": "^4.1.0",
    "prop-types": "^15.7.2",
    "random-birthday": "^1.0.1"
  }
}
