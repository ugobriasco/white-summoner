{
  "name": "summoner",
  "version": "1.0.0",
  "description": "persona generator",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha lib/*/__tests/*.spec.js",
    "test:integration": "mocha tests/integration/*.spec.js",
    "build:deptree": "depcruise --exclude '^node_modules' --output-type dot lib | dot -T svg > dependencygraph.svg"
